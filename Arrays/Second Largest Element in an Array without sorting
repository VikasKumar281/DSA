class Solution {
public:
    int secondLargestElement(vector<int>& nums) {
        int largest =  nums[0];
        int n = nums.size();
        for(int i =0 ; i<n ; i++){
            if(largest < nums[i]){
                largest = nums[i];
            }
        }

        int secondLargest = -1;
        for(int i = 0;i<n;i++){
            if(secondLargest < nums[i] && nums[i] != largest){
                secondLargest = nums[i];
            }
        }

        return secondLargest;
    }
};
