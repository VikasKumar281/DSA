class Solution {
public:
    void moveZeroes(vector<int>& arr) {
        int n = arr.size();
        vector<int > temp(n);
        if(n==1){
            temp[0] = arr[0];
        }

        int j =0;
        for(int i =0;i<n;i++){
            if(arr[i] !=0){
              temp[j] = arr[i];
              j++;
            }
        }
        int k = temp.size();

        for(int i = k;i<n;i++){
            temp[i] = 0;
        }

        arr = temp;
    }
};

// T.C. = S.C. = O(n)
