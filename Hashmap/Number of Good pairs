class Solution {
public:
    int numIdenticalPairs(vector<int>& nums) {
        int result =0;
        int n = nums.size();
       
        map<int,int>mp;


        for(int i =0;i<n;i++){
            result = result+mp[nums[i]];
            mp[nums[i]]++;
        }

        return result;
    }
};


// T.C. = O(N) = S.C.
